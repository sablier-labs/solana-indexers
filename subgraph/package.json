{
  "name": "lockup",
  "license": "Apache-2.0",
  "scripts": {
    "build": "graph build",
    "codegen": "yarn codegen-assembly && yarn codegen-proto",
    "codegen-assembly": "graph codegen",
    "codegen-proto": "buf generate --type=\"substreams.v1.program.Data\" ../lockup-v0.1.0.spkg#format=bin; echo ''; echo '^^^ You can safely ignore warnings about \"non-conform Protobuf generation specification\": they come from the assemblyscript buf plugin'; echo ''",
    "stream": "yarn stream-template-yaml && yarn stream-build",
    "stream-build": "cd ../ && substreams build",
    "stream-template-yaml": "mustache ../constants/$CLUSTER.json ../substreams.mustache > ../substreams.yaml",
    "stream-gui:devnet": "substreams gui -e devnet.sol.streamingfast.io:443 ../substreams.yaml",
    "gui:devnet": "CLUSTER=devnet yarn setup && yarn stream-gui:devnet",
    "setup:devnet": "CLUSTER=devnet yarn setup",
    "setup": "yarn stream && yarn codegen",
    "studio-deploy": "graph deploy --studio solana-lockup-experimental",
    "studio-auth": "graph auth --studio"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.73.0",
    "@graphprotocol/graph-ts": "^0.34.0",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "as-proto": "^1.3.0",
    "assemblyscript-json": "^1.1.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "protoc-gen-js": "^3.21.2",
    "protocol-buffers": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@stepd/as-proto-gen": "^1.3.3",
    "@types/google-protobuf": "^3.15.12",
    "matchstick-as": "^0.6.0",
    "mustache": "^4.2.0"
  }
}
