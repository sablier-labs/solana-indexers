{
  "name": "lockup",
  "version": "0.2.0",
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.73.0",
    "@graphprotocol/graph-ts": "^0.34.0",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "as-proto": "^1.3.0",
    "assemblyscript-json": "^1.1.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "protoc-gen-js": "^3.21.2",
    "protocol-buffers": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@stepd/as-proto-gen": "^1.3.3",
    "@types/google-protobuf": "^3.15.12",
    "matchstick-as": "^0.6.0",
    "mustache": "^4.2.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "_codegen": "yarn _codegen-yaml && yarn _codegen-constants-rs && yarn _codegen-constants-assembly && yarn _codegen-proto-rs && yarn substream-build && yarn _codegen-proto-assembly && yarn _codegen-assembly",
    "_codegen-assembly": "graph codegen",
    "_codegen-constants-assembly": "mustache ../constants/$CLUSTER.json ../templates/subgraph-constants.mustache > ../subgraph/generated/env.ts",
    "_codegen-constants-rs": "mkdir -p ../src/generated && mustache ../constants/$CLUSTER.json ../templates/src-constants.mustache > ../src/generated/constants.rs",
    "_codegen-proto-assembly": "buf generate --type=\"substreams.v1.program.Data\" ../lockup-v0.2.0.spkg#format=bin > /dev/null 2>&1; echo ''; echo '^^^ $ buf generate output silenced'; echo ''",
    "_codegen-proto-rs": "cd ../ && substreams protogen ./substreams.yaml",
    "_codegen-yaml": "mustache ../constants/$CLUSTER.json ../templates/substreams.mustache > ../substreams.yaml",
    "_subgraph-deploy": "graph deploy --studio $NAME",
    "deploy:devnet": "CLUSTER=devnet NAME=sablier-solana-lockup-experimental-1-0-1 yarn _subgraph-deploy",
    "setup:devnet": "CLUSTER=devnet yarn _codegen",
    "subgraph-auth": "graph auth --studio",
    "subgraph-build": "graph build",
    "substream-auth": "substreams auth && echo '⚠️ Please manually run the export line pasted in .substreams.env in your terminal to add the variable to your environment'",
    "substream-build": "substreams build",
    "substream-gui:devnet": "yarn substream-build && substreams gui -e devnet.sol.streamingfast.io:443 ../substreams.yaml",
    "substream-run:devnet": "yarn substream-build && substreams run -e devnet.sol.streamingfast.io:443 ../substreams.yaml"
  }
}
