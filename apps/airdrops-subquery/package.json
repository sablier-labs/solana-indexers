{
  "name": "@sablier/solana-indexers-airdrops-subquery",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "_codegen": "yarn _codegen-constants && yarn _codegen-adapter && yarn _codegen-types",
    "_codegen-types": "yarn subql codegen",
    "_codegen-constants": "mkdir -p ./src/generated && mustache ../../packages/constants/$CLUSTER.json ../../packages/templates/constants.ts.mustache > ./src/generated/env.ts",
    "_codegen-adapter": "mkdir -p ./src/generated && mustache ../../packages/constants/$CLUSTER.json ../../packages/templates/airdrops-subquery/adapters.ts.mustache > ./src/generated/adapters.ts",
    "deploy-traceye:devnet": "yarn setup:devnet && yarn run traceye subquery:deploy --url https://app.traceye.io/shared-api/subquery/a80348ee7a697e3459a356bfbe868117c64d75585baabca4/deploy/node --ipfs https://app.traceye.io/shared-api/subquery/a80348ee7a697e3459a356bfbe868117c64d75585baabca4/ipfs/node sablier-solana-airdrops-devnet",
    "dev": "yarn subquery-run-clean:devnet",
    "publish:devnet": "yarn setup:devnet && yarn subql publish",
    "setup:devnet": "CLUSTER=devnet yarn _codegen",
    "subquery-build": "yarn setup:devnet && yarn subql build",
    "subquery-play": "yarn subql build && docker compose pull && docker compose up --remove-orphans",
    "subquery-run:devnet": "yarn setup:devnet && yarn subql build && docker compose pull && docker compose up --remove-orphans",
    "subquery-run-clean:devnet": "rm -rf ./.data && yarn run subquery-run:devnet"
  },
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "razgraf",
  "license": "MIT",
  "dependencies": {
    "@solana/kit": "^3.0.2",
    "@subql/common-solana": "^1.1.1",
    "@subql/types-solana": "^1.1.0",
    "abort-controller": "^3.0.0",
    "event-target-shim": "^6.0.2",
    "fastestsmallesttextencoderdecoder": "^1.0.22"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.6",
    "@subql/cli": "^6.2.2",
    "@traceye-tech/cli": "^2.0.0",
    "@types/events": "^3",
    "@types/fastestsmallesttextencoderdecoder": "^1",
    "@types/node-fetch": "latest",
    "codama": "^1.3.4",
    "dotenv": "latest",
    "mustache": "^4.2.0",
    "typescript": "5.4.4"
  }
}
